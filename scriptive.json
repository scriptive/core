            "(desktop|web|chrome|ios)": false
          },
          "root": {
            "(android|app)": true,
            "(web)": false
          }
        }
      }
    },
    "default": {
      "dev": {
        "main": "default"
      },
      "dist": {
        "type": "web",
        "file": {
          "css": {
            "(app)": false
          },
          "js": {
            "(app)": false
          },
          "root": {
            "(default|web)": true,
            "(app)": false
          }
        }
      }
    },
    "web": {
      "dev": {
        "main": "default"
      },
      "dist": {
        "type": "web",
        "file": {
          "css": {
            "(app)": false
          },
          "js": {
            "(app)": false
          },
          "root": {
